    "eslint": "^3.19.0",
    "eslint-config-laxarjs": "^2.0.0",
    "karma": "1.6.0",
    "laxar": "^2.0.0",
    "laxar-infrastructure": "^2.0.0",
    "reflect-metadata": "^0.1.8",
    "rxjs": "~5.0.3",
    "ts-loader": "^2.0.3",
    "typescript": "^2.1.4",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.2",
    "webpack-jasmine-html-runner-plugin": "^0.7.0",
    "zone.js": "0.7.4"
