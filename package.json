  "description": "Write LaxarJS widgets and controls using Angular 2",
  "main": "laxar-angular2-adapter",
  "scripts": {
    "test": "npm run karma && npm run eslint",
    "start": "webpack && webpack-dev-server --inline",
    "karma": "npm run karma-main && npm run karma-directives",
    "karma-main": "karma start karma.config.js spec/spec-runner.js",
    "karma-directives": "karma start karma.config.js lib/directives/spec/spec-runner.js",
    "eslint": "eslint *.config.js lib spec"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LaxarJS/laxar-angular2-adapter.git"
  },
